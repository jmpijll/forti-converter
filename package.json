    "tailwindcss": "^3.4.0",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^3.3.1",
    "@sveltejs/kit": "^2.20.7",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/svelte": "^4.2.3",
    "@testing-library/user-event": "^14.5.2",
    "@tsconfig/svelte": "^5.0.4",
    "@types/express": "^4.17.0",
    "@types/jest": "^29.5.14",
