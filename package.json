{
  "name": "forti-converter",
  "version": "1.0.0",
  "private": true,
  "description": "A modern web application for converting FortiAnalyzer traffic logs into FortiGate CLI configuration scripts",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:watch": "vitest watch",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
